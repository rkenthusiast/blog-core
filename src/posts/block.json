{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "create-block/posts",
	"version": "0.1.0",
	"title": "Homepage - Posts",
	"category": "widgets",
	"icon": "smiley",
	"description": "Example Dynamic new block.",
	"attributes": {
		"sectionTitle": {
			"type": "string",
			"default": "Latest Posts"
		},
		"numberOfPosts": {
			"type": "number",
			"default": 9
		},
		"viewMoreText": {
			"type": "string",
			"default": "View All Post"
		},
		"postsToShow": {
			"type": "number",
			"default": 3
		  },
		  "loadMoreText": {
			"type": "string",
			"default": "View More"
		  }
	},
	"supports": {
		"html": false
	},
	"textdomain": "posts",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
